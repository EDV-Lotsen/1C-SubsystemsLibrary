	<Attributes>
		<Attribute name="ExternalUserList" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>External user list</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	ExternalUsers.Ref,
	ExternalUsers.DeletionMark,
	ExternalUsers.Predefined,
	ExternalUsers.Description,
	CASE
		WHEN ExternalUsers.DeletionMark
			THEN 6
		WHEN ExternalUsers.InfoBaseUserID = &amp;EmptyUUID
			THEN 10
		ELSE 7
	END AS PictureNumber
FROM
	Catalog.ExternalUsers AS ExternalUsers
WHERE
	(&amp;AnyAuthorizationObjectType
			OR &amp;AuthorizationObjectType = VALUETYPE(ExternalUsers.AuthorizationObject))
	AND (&amp;SelectHierarchy
				AND TRUE IN
					(SELECT
						TRUE
					FROM
						InformationRegister.UserGroupContent AS UserGroupContent
					WHERE
						UserGroupContent.User = ExternalUsers.Ref
						AND UserGroupContent.UserGroup = &amp;ExternalUserGroup)
			OR (NOT &amp;SelectHierarchy)
				AND TRUE IN
					(SELECT
						TRUE
					FROM
						Catalog.ExternalUserGroups.Content AS ExternalUserGroupContent
					WHERE
						ExternalUserGroupContent.ExternalUser = ExternalUsers.Ref
							AND ExternalUserGroupContent.Ref = &amp;ExternalUserGroup))</QueryText>
				<MainTable>Catalog.ExternalUsers</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:use>false</dcsset:use>
							<dcsset:left xsi:type="dcscor:Field">DeletionMark</dcsset:left>
							<dcsset:comparisonType>Equal</dcsset:comparisonType>
							<dcsset:right xsi:type="xs:boolean">false</dcsset:right>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>Description</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:item>
							<dcsset:selection/>
							<dcsset:filter>
								<dcsset:item xsi:type="dcsset:FilterItemComparison">
									<dcsset:left xsi:type="dcscor:Field">Ref.NotValid</dcsset:left>
									<dcsset:comparisonType>Equal</dcsset:comparisonType>
									<dcsset:right xsi:type="xs:boolean">true</dcsset:right>
								</dcsset:item>
							</dcsset:filter>
							<dcsset:appearance>
								<dcscor:item xsi:type="dcsset:SettingsParameterValue">
									<dcscor:parameter>TextColor</dcscor:parameter>
									<dcscor:value xsi:type="v8ui:Color">style:InaccessibleCellTextColor</dcscor:value>
								</dcscor:item>
							</dcsset:appearance>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
		<Attribute name="ExternalUserGroups" id="2">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>External user groups</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<UseAlways>
				<Field>ExternalUserGroups.Parent</Field>
				<Field>ExternalUserGroups.AuthorizationObjectType</Field>
				<Field>ExternalUserGroups.AllAuthorizationObjects</Field>
			</UseAlways>
			<FunctionalOptions>
				<Item>FunctionalOption.UseUserGroups</Item>
			</FunctionalOptions>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	ExternalUserGroups.Ref,
	ExternalUserGroups.DeletionMark,
	ExternalUserGroups.Predefined,
	ExternalUserGroups.Parent,
	ExternalUserGroups.Description,
	CASE
		WHEN ExternalUserGroups.DeletionMark
			THEN 8
		ELSE 9
	END AS PictureNumber,
	ExternalUserGroups.AuthorizationObjectType,
	ExternalUserGroups.AllAuthorizationObjects
FROM
	Catalog.ExternalUserGroups AS ExternalUserGroups
WHERE
	(ExternalUserGroups.AuthorizationObjectType = UNDEFINED
			OR &amp;AnyAuthorizationObjectType
				OR &amp;AuthorizationObjectType = VALUETYPE(ExternalUserGroups.AuthorizationObjectType))</QueryText>
