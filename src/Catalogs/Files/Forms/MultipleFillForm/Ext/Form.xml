			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogFileFolders.Ref,
	CatalogFileFolders.DeletionMark,
	CatalogFileFolders.Predefined,
	CatalogFileFolders.Parent,
	CatalogFileFolders.Code,
	CatalogFileFolders.Description,
	CatalogFileFolders.Details,
	CatalogFileFolders.Responsible,
	CatalogFileFolders.CreationDate,
	CatalogFileFolders.AdditionalAttributes.(
		Ref,
		LineNumber,
		Property,
		Value,
		TextString
	),
	CASE
		WHEN CatalogFileFolders.DeletionMark = TRUE
			THEN 1
		ELSE 0
	END AS PictureIndex
FROM
	Catalog.FileFolders AS CatalogFileFolders</QueryText>
				<MainTable>Catalog.FileFolders</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:left xsi:type="dcscor:Field">Ref</dcsset:left>
							<dcsset:comparisonType>InHierarchy</dcsset:comparisonType>
							<dcsset:right xsi:type="dcscor:DesignTimeValue">Catalog.FileFolders.Templates1</dcsset:right>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
