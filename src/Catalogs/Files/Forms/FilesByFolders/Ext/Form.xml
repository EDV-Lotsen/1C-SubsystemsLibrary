			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	FileFolders.Ref,
	FileFolders.DeletionMark,
	FileFolders.Predefined,
	FileFolders.Parent,
	FileFolders.Code,
	FileFolders.Description,
	FileFolders.Details,
	FileFolders.Responsible,
	FileFolders.CreationDate,
	FileFolders.Presentation,
	CASE
