	<Attributes>
		<Attribute name="PropertiesTree" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Properties tree</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<UseAlways>
				<Field>PropertiesTree.InfoPictureNo</Field>
				<Field>PropertiesTree.Comment</Field>
				<Field>1/41</Field>
				<Field>1/39</Field>
				<Field>1/36</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Main.Ref,
	CASE
		WHEN Main.Ref IN (&amp;arrSelected)
			THEN TRUE
		ELSE FALSE
	END AS Selected,
	CASE
		WHEN Main.IsFolder
			THEN 0
		WHEN Main.IsAdditionalData
			THEN 2
		ELSE 1
	END + CASE
		WHEN Main.DeletionMark
			THEN 3
		ELSE 0
	END AS InfoPictureNo,
	Main.IsFolder,
	Main.Comment AS Comment,
	Main.IsAdditionalData AS IsAdditionalData
FROM
	ChartOfCharacteristicTypes.AdditionalDataAndAttributes AS Main</QueryText>
				<MainTable>ChartOfCharacteristicTypes.AdditionalDataAndAttributes</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
