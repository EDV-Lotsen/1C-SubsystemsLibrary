	<Attributes>
		<Attribute name="UserList" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>User list</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Users.Ref,
	Users.DeletionMark,
	Users.Predefined,
	Users.Description,
	CASE
		WHEN Users.DeletionMark
			THEN 0
		WHEN Users.InfoBaseUserID = &amp;EmptyUUID
			THEN 4
		ELSE 1
	END AS PictureNumber
FROM
	Catalog.Users AS Users
WHERE
	Users.Ref &lt;&gt; &amp;UnspecifiedUser
	AND (&amp;SelectHierarchy
				AND TRUE IN
					(SELECT
						TRUE
					FROM
						InformationRegister.UserGroupContent AS UserGroupContent
					WHERE
						UserGroupContent.User = Users.Ref
						AND UserGroupContent.UserGroup = &amp;UserGroup)
			OR NOT &amp;SelectHierarchy
				AND TRUE IN
					(SELECT
						TRUE
					FROM
						Catalog.UserGroups.Content AS UserGroupContent
					WHERE
						UserGroupContent.User = Users.Ref
						AND UserGroupContent.Ref = &amp;UserGroup))
	AND (&amp;SeparationDisabled 
		OR (NOT TRUE IN
				(SELECT
					TRUE
				FROM
					InformationRegister.SharedUsers AS SharedUsers
				WHERE
						SharedUsers.InfoBaseUserID = Users.InfoBaseUserID)))</QueryText>
				<MainTable>Catalog.Users</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:use>false</dcsset:use>
							<dcsset:left xsi:type="dcscor:Field">DeletionMark</dcsset:left>
							<dcsset:comparisonType>Equal</dcsset:comparisonType>
							<dcsset:right xsi:type="xs:boolean">false</dcsset:right>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>Description</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:item>
							<dcsset:selection/>
							<dcsset:filter>
								<dcsset:item xsi:type="dcsset:FilterItemComparison">
									<dcsset:left xsi:type="dcscor:Field">Ref.NotValid</dcsset:left>
									<dcsset:comparisonType>Equal</dcsset:comparisonType>
									<dcsset:right xsi:type="xs:boolean">true</dcsset:right>
								</dcsset:item>
							</dcsset:filter>
							<dcsset:appearance>
								<dcscor:item xsi:type="dcsset:SettingsParameterValue">
									<dcscor:parameter>TextColor</dcscor:parameter>
									<dcscor:value xsi:type="v8ui:Color">style:InaccessibleCellTextColor</dcscor:value>
								</dcscor:item>
							</dcsset:appearance>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
		<Attribute name="UserGroups" id="2">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>User groups</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<UseAlways>
				<Field>UserGroups.Parent</Field>
			</UseAlways>
			<FunctionalOptions>
				<Item>FunctionalOption.UseUserGroups</Item>
			</FunctionalOptions>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	UserGroupsCatalog.Ref,
	UserGroupsCatalog.DeletionMark,
	UserGroupsCatalog.Predefined,
	UserGroupsCatalog.Parent,
	UserGroupsCatalog.Description,
	CASE
		WHEN UserGroupsCatalog.DeletionMark
			THEN 2
		ELSE 3
	END AS PictureNumber
FROM
		Catalog.UserGroups AS UserGroupsCatalog</QueryText>
