									<dcsset:comparisonType>Equal</dcsset:comparisonType>
									<dcsset:right xsi:type="dcscor:DesignTimeValue">Enum.ProductKinds.Service</dcsset:right>
								</dcsset:item>
							</dcsset:filter>
							<dcsset:appearance>
