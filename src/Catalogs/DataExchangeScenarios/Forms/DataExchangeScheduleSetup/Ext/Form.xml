	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.UseScheduledJob</Field>
				<Field>List.UseImportFlag</Field>
				<Field>List.UseExportFlag</Field>
				<Field>List.UsageFlag</Field>
				<Field>List.Ref</Field>
				<Field>1/11</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	DataExchangeScenarios.Ref,
	DataExchangeScenarios.UseScheduledJob,
	DataExchangeScenarios.Description,
	ISNULL(NestedSelect.UsageFlag, False) AS UsageFlag,
	ISNULL(NestedSelect.UseImportFlag, False) AS UseImportFlag,
	ISNULL(NestedSelect.UseExportFlag, False) AS UseExportFlag
FROM 
	Catalog.DataExchangeScenarios AS DataExchangeScenarios
LEFT JOIN
	(SELECT
		DataExchangeScenarioExchangeSettings.Ref,
		DataExchangeScenarioExchangeSettings.Ref.UseScheduledJob,
		DataExchangeScenarioExchangeSettings.Ref.Description,
		MAX(CASE WHEN DataExchangeScenarioExchangeSettings.InfoBaseNode = &amp;InfoBaseNode 
		THEN True 
		ELSE False
		END) AS UsageFlag,
		MAX(CASE WHEN DataExchangeScenarioExchangeSettings.InfoBaseNode = &amp;InfoBaseNode 
		AND DataExchangeScenarioExchangeSettings.CurrentAction = VALUE(Enum.ActionsOnExchange.DataImport) 
		THEN True 
		ELSE False
		END) AS UseImportFlag,
		MAX(CASE WHEN DataExchangeScenarioExchangeSettings.InfoBaseNode = &amp;InfoBaseNode 
		AND DataExchangeScenarioExchangeSettings.CurrentAction = VALUE(Enum.ActionsOnExchange.DataExport) 
		THEN True 
		ELSE False
		END) AS UseExportFlag
	FROM
		Catalog.DataExchangeScenarios.ExchangeSettings AS DataExchangeScenarioExchangeSettings
	WHERE
		NOT DataExchangeScenarioExchangeSettings.Ref.DeletionMark
	GROUP BY
		DataExchangeScenarioExchangeSettings.Ref,
		DataExchangeScenarioExchangeSettings.Ref.UseScheduledJob,
		DataExchangeScenarioExchangeSettings.Ref.Description
		) AS NestedSelect

On NestedSelect.Ref = DataExchangeScenarios.Ref
WHERE
	NOT DataExchangeScenarios.DeletionMark
ORDER BY
	DataExchangeScenarios.Description
</QueryText>
