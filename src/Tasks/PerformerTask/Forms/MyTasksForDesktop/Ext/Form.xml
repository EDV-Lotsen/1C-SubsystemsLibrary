	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Subject</Field>
				<Field>List.Ref</Field>
				<Field>List.BusinessProcess</Field>
				<Field>List.AcceptedForExecution</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	TasksByPerformer.Ref,
	TasksByPerformer.DeletionMark,
	TasksByPerformer.Number,
	TasksByPerformer.Date,
	TasksByPerformer.BusinessProcess,
	TasksByPerformer.RoutePoint,
	TasksByPerformer.Description,
	TasksByPerformer.Executed,
	TasksByPerformer.DueDate,
	CASE
		WHEN TasksByPerformer.StartDate = &amp;EmptyDate
			THEN &amp;BegOfDay
		ELSE TasksByPerformer.StartDate
	END AS StartDate,
	TasksByPerformer.CompletionDate,
	TasksByPerformer.Importance,
	TasksByPerformer.Details,
	TasksByPerformer.PerformerRole,
	TasksByPerformer.Performer,
	TasksByPerformer.Presentation,
	TasksByPerformer.SubjectString,
	TasksByPerformer.Author,
	TasksByPerformer.AcceptedForExecution,
	CASE
		WHEN TasksByPerformer.Importance = VALUE(Enum.TaskImportanceVariants.Low)
			THEN 0
		WHEN TasksByPerformer.Importance = VALUE(Enum.TaskImportanceVariants.High)
			THEN 2
		ELSE 1
	END AS ImportancePicture,
	TasksByPerformer.Subject,
	CASE
		WHEN TasksByPerformer.DueDate &lt;= &amp;CurrentDate
			THEN &amp;Overdue
		WHEN TasksByPerformer.DueDate &gt; &amp;CurrentDate AND TasksByPerformer.DueDate &lt;= &amp;EndOfDay
			THEN &amp;Today
		WHEN TasksByPerformer.DueDate &gt; &amp;EndOfDay AND TasksByPerformer.DueDate &lt;= &amp;EndOfWeek
			THEN &amp;ThisWeek
		WHEN TasksByPerformer.DueDate &gt; &amp;EndOfWeek AND TasksByPerformer.DueDate &lt;= &amp;EndOfNextWeek
			THEN &amp;NextWeek
		ELSE &amp;Later
	END AS GroupDeadline
FROM
	Task.PerformerTask.TasksByExecutive AS TasksByPerformer
WHERE
	TasksByPerformer.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Active)

</QueryText>
