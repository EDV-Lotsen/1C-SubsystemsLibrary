	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.SubjectString</Field>
				<Field>List.Subject</Field>
				<Field>List.Ref</Field>
				<Field>List.BusinessProcess</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	TaskPerformerTask.Ref,
	TaskPerformerTask.DeletionMark,
	TaskPerformerTask.Number,
	TaskPerformerTask.Date,
	TaskPerformerTask.BusinessProcess,
	TaskPerformerTask.RoutePoint,
	TaskPerformerTask.Description,
	TaskPerformerTask.Executed,
	CASE
		WHEN TaskPerformerTask.Importance = VALUE(Enum.TaskImportanceVariants.Low)
			THEN 0
		WHEN TaskPerformerTask.Importance = VALUE(Enum.TaskImportanceVariants.High)
			THEN 2
		ELSE 1
	END AS ImportancePicture,
	TaskPerformerTask.Importance,
	TaskPerformerTask.CompletionDate,
	TaskPerformerTask.Author,
	TaskPerformerTask.Description,
	TaskPerformerTask.DueDate,
	TaskPerformerTask.ExecutionResult,
	TaskPerformerTask.SubjectString,
	TaskPerformerTask.Subject,
	TaskPerformerTask.Performer,
	TaskPerformerTask.PerformerRole,
	TaskPerformerTask.MainAddressingObject,
	TaskPerformerTask.AdditionalAddressingObject,
	TaskPerformerTask.AcceptedForExecution,
	CASE
		WHEN TaskPerformerTask.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Stopped)
			THEN True
		ELSE False
	END AS Stopped
FROM
	Task.PerformerTask AS TaskPerformerTask
WHERE
	(&amp;Performer IS NULL 
			OR TaskPerformerTask.Performer = &amp;Performer
			OR TRUE IN
				(SELECT
					TRUE
				FROM
					InformationRegister.TaskPerformers AS TaskPerformers
				WHERE
					TaskPerformerTask.PerformerRole = TaskPerformers.PerformerRole
					AND TaskPerformerTask.MainAddressingObject = TaskPerformers.MainAddressingObject
					AND TaskPerformerTask.AdditionalAddressingObject = TaskPerformers.AdditionalAddressingObject
					AND TaskPerformers.Performer = &amp;Performer))</QueryText>
				<MainTable>Task.PerformerTask</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
