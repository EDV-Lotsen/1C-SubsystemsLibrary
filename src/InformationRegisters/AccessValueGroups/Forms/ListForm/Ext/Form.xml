	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.PictureNumberAccessValues</Field>
				<Field>List.DefaultPicture</Field>
				<Field>List.AccessValueGroup</Field>
				<Field>List.AccessValue</Field>
				<Field>1/6</Field>
				<Field>1/5</Field>
				<Field>1/4</Field>
				<Field>1/2</Field>
				<Field>1/1</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CASE
		WHEN VALUETYPE(AccessValueGroups.AccessValue) = TYPE(Catalog.Users)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValue AS Catalog.Users).DeletionMark
						THEN 0
					ELSE 1
				END
		WHEN VALUETYPE(AccessValueGroups.AccessValue) = TYPE(Catalog.UserGroups)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValue AS Catalog.UserGroups).DeletionMark
						THEN 2
					ELSE 3
				END
		WHEN VALUETYPE(AccessValueGroups.AccessValue) = TYPE(Catalog.ExternalUsers)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValue AS Catalog.ExternalUsers).DeletionMark
						THEN 6
					ELSE 7
				END
		WHEN VALUETYPE(AccessValueGroups.AccessValue) = TYPE(Catalog.ExternalUserGroups)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValue AS Catalog.ExternalUserGroups).DeletionMark
						THEN 8
					ELSE 9
				END
		ELSE -1
	END AS PictureNumberAccessValues, AccessValueGroups.AccessValue,
	CASE
		WHEN VALUETYPE(AccessValueGroups.AccessValueGroup) = TYPE(Catalog.Users)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValueGroup AS Catalog.Users).DeletionMark
						THEN 0
					ELSE 1
				END
		WHEN VALUETYPE(AccessValueGroups.AccessValueGroup) = TYPE(Catalog.UserGroups)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValueGroup AS Catalog.UserGroups).DeletionMark
						THEN 2
					ELSE 3
				END
		WHEN VALUETYPE(AccessValueGroups.AccessValueGroup) = TYPE(Catalog.ExternalUsers)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValueGroup AS Catalog.ExternalUsers).DeletionMark
						THEN 6
					ELSE 7
				END
		WHEN VALUETYPE(AccessValueGroups.AccessValueGroup) = TYPE(Catalog.ExternalUserGroups)
			THEN CASE
					WHEN CAST(AccessValueGroups.AccessValueGroup AS Catalog.ExternalUserGroups).DeletionMark
						THEN 8
					ELSE 9
				END
		ELSE -1
	END AS PictureNumberAccessValueGroups,
	AccessValueGroups.AccessValueGroup,
	AccessValueGroups.DataGroup
FROM
	InformationRegister.AccessValueGroups AS AccessValueGroups</QueryText>
				<MainTable>InformationRegister.AccessValueGroups</MainTable>
				<ListSettings>
					<dcsset:filter>
