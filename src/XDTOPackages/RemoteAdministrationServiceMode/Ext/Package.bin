<package xmlns="http://v8.1c.ru/8.1/xdto" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" targetNamespace="http://1c-dn.com/SaaS/RemoteAdministration/App">
	<import namespace="http://v8.1c.ru/8.1/data/core"/>
	<import namespace="http://v8.1c.ru/8.1/data/enterprise"/>
	<import namespace="http://v8.1c.ru/8.1/data/enterprise/current-config"/>
	<import namespace="http://1c-dn.com/SaaS/RemoteAdministration/Messages"/>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="UpdateUser" base="d2p1:Message">
		<property name="Name" type="xs:string"/>
		<property name="FullName" type="xs:string"/>
		<property name="StoredPasswordValue" type="xs:string"/>
		<property name="Language" type="xs:string"/>
		<property name="Email" lowerBound="0">
			<typeDef xsi:type="ValueType" base="xs:string" variety="Atomic">
				<pattern>[^@]+@[^\.]+\..+</pattern>
			</typeDef>
		</property>
		<property name="DataArea" type="xs:decimal"/>
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="ServiceUserID" type="d3p1:UUID"/>
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="DataAreasUserID" type="d3p1:UUID" lowerBound="0"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="PrepareDataArea" base="d2p1:Message">
		<property name="DataArea" type="xs:decimal"/>
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="DataFileIdentifier" type="d3p1:UUID" lowerBound="0"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="DeleteDataArea" base="d2p1:Message">
		<property name="DataArea" type="xs:decimal"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="SetAccessToDataArea" base="d2p1:Message">
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="ServiceUserID" type="d3p1:UUID"/>
		<property name="DataArea" type="xs:decimal"/>
		<property name="Value" type="xs:boolean"/>
		<property name="Name" type="xs:string" lowerBound="0"/>
		<property name="StoredPasswordValue" type="xs:string" lowerBound="0"/>
		<property name="Language" type="xs:string" lowerBound="0"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="SetServiceManagerEndPoint" base="d2p1:Message"/>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="SetInfoBaseParameters" base="d2p1:Message">
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="Parameters" type="d3p1:Structure"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="SetDataAreaParameters" base="d2p1:Message">
		<property name="DataArea" type="xs:decimal"/>
		<property name="TimeZone" type="xs:string"/>
		<property name="Description" type="xs:string"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="SetDataAreaFullAccess" base="d2p1:Message">
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="ServiceUserID" type="d3p1:UUID"/>
		<property name="DataArea" type="xs:decimal"/>
		<property name="Value" type="xs:boolean"/>
	</objectType>
	<objectType xmlns:d2p1="http://1c-dn.com/SaaS/RemoteAdministration/Messages" name="SetDefaultUserRights" base="d2p1:Message">
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="ServiceUserID" type="d3p1:UUID"/>
		<property name="DataArea" type="xs:decimal"/>
	</objectType>
</package>