		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED DISTINCT
	Orders.Ref,
	Orders.MainAddressingObject,
	Orders.AdditionalAddressingObject,
	Orders.PerformerRole,
	Orders.PerformerPresenceFlag,
	Orders.Author AS Author,
	Orders.Quantity
FROM
	(SELECT
		Orders.Coordinator AS Coordinator,
		Orders.MainAddressingObject AS MainAddressingObject,
		Orders.AdditionalAddressingObject AS AdditionalAddressingObject,
		Orders.PerformerRole AS PerformerRole,
		Orders.Author AS Author,
		Orders.Ref AS Ref,
		Orders.Quantity AS Quantity,
		Orders.PerformerPresenceFlag AS PerformerPresenceFlag
	FROM
		(SELECT
			CASE
				WHEN TaskPerformers2.Performer IS Not NULL 
						AND TaskPerformers2.Performer &lt;&gt; VALUE(Catalog.Users.EmptyRef)
					THEN TaskPerformers2.Performer
				ELSE TaskPerformers4.Performer
			END AS Coordinator,
			Orders.MainAddressingObject AS MainAddressingObject,
			Orders.AdditionalAddressingObject AS AdditionalAddressingObject,
			CASE
				WHEN Orders.Performer IS Not NULL 
						AND Orders.Performer &lt;&gt; VALUE(Catalog.Users.EmptyRef)
					THEN Orders.Performer
				ELSE Orders.PerformerRole
			END AS PerformerRole,
			Orders.Author AS Author,
			Orders.Ref AS Ref,
			1 AS Quantity,
			CASE
				WHEN Orders.Performer = VALUE(Catalog.Users.EmptyRef)
						AND TaskPerformers.Performer IS NULL 
					THEN 0
				ELSE 1
			END AS PerformerPresenceFlag
		FROM
			Task.PerformerTask AS Orders
				LEFT JOIN
InformationRegister.TaskPerformers AS TaskPerformers
				ON (TaskPerformers.PerformerRole = Orders.PerformerRole)
					AND (TaskPerformers.MainAddressingObject = Orders.MainAddressingObject)
					AND (TaskPerformers.AdditionalAddressingObject = Orders.AdditionalAddressingObject)
				LEFT JOIN
InformationRegister.TaskPerformers AS TaskPerformers2
				ON (TaskPerformers2.PerformerRole = VALUE(Catalog.PerformerRoles.ResponsibleForCompletionControl))
					AND (TaskPerformers2.MainAddressingObject = Orders.MainAddressingObject)
				LEFT JOIN
InformationRegister.TaskPerformers AS TaskPerformers4
				ON (TaskPerformers4.PerformerRole = VALUE(Catalog.PerformerRoles.ResponsibleForCompletionControl))
					AND (TaskPerformers4.MainAddressingObject = UNDEFINED)
		WHERE
			Orders.Executed = FALSE
			AND Orders.DeletionMark = FALSE
			AND Orders.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Active)
		
		GROUP BY
			Orders.Ref,
			Orders.Author,
			Orders.MainAddressingObject,
			Orders.PerformerRole,
			TaskPerformers.Performer,
			TaskPerformers2.Performer,
			TaskPerformers4.Performer,
			Orders.AdditionalAddressingObject,
			CASE
				WHEN Orders.Performer = VALUE(Catalog.Users.EmptyRef)
						AND TaskPerformers.Performer IS NULL 
					THEN 0
				ELSE 1
			END) AS Orders
	{WHERE
		(&amp;Coordinator = VALUE(Catalog.Users.EmptyRef)
				OR &amp;Coordinator &lt;&gt; VALUE(Catalog.Users.EmptyRef)
					AND Order.Coordinator = &amp;Coordinator)}) AS Orders</query>
	</dataSet>
