			<field>Confirmed</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Result</dataPath>
			<field>Result</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	Job.Ref,
	Job.Date,
	Job.Completed,
	Job.Started,
	Job.Description,
	Job.Importance,
	Job.DueDate,
	Job.Subject,
	Job.Author,
	Job.CompletionDate,
	Job.IterationNumber,
	Job.Completed,
	Job.Confirmed,
	CASE
		WHEN Job.Completed
				AND Job.Completed
			THEN 0
		WHEN Job.Completed
				AND (Not Job.Completed)
			THEN 1
		ELSE 2
	END AS Result
FROM
	BusinessProcess.Job AS Job
WHERE
	Job.SourceTask = VALUE(Task.PerformerTask.EmptyRef)	
{WHERE
	(Job.Completed = &amp;Completed),
	(Job.CompletionDate &gt;= &amp;PeriodBeginning
			OR Job.CompletionDate = DATETIME(1, 1, 1))}</query>
