		<dataSource>DataSource1</dataSource>
		<query>SELECT
	GeneralJournalBalance.ExtDimension1 AS Company,
	GeneralJournalBalance.ExtDimension1.Description AS CompanyID,
	GeneralJournalBalance.ExtDimension2 AS Document,
	GeneralJournalBalance.Currency,
	CASE
		WHEN GeneralJournalBalance.ExtDimension2 REFS Document.SalesReturn
			THEN PRESENTATION(GeneralJournalBalance.ExtDimension2.ReturnType)
		ELSE ""
	END AS ReturnType,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= 0
			THEN GeneralJournalBalance.AmountBalance
		ELSE 0
	END AS D0_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= 0
			THEN GeneralJournalBalance.AmountRCBalance
		ELSE 0
	END AS D0,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= &amp;Interval1
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; 0
			THEN GeneralJournalBalance.AmountBalance
		ELSE 0
	END AS D1_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= &amp;Interval1
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; 0
			THEN GeneralJournalBalance.AmountRCBalance
		ELSE 0
	END AS D1,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= &amp;Interval2
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; &amp;Interval1
			THEN GeneralJournalBalance.AmountBalance
		ELSE 0
	END AS D2_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= &amp;Interval2
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; &amp;Interval1
			THEN GeneralJournalBalance.AmountRCBalance
		ELSE 0
	END AS D2,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= &amp;Interval3
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; &amp;Interval2
			THEN GeneralJournalBalance.AmountBalance
		ELSE 0
	END AS D3_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &lt;= &amp;Interval3
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; &amp;Interval2
			THEN GeneralJournalBalance.AmountRCBalance
		ELSE 0
	END AS D3,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; &amp;Interval3
			THEN GeneralJournalBalance.AmountBalance
		ELSE 0
	END AS D4_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.DueDate, &amp;Period, DAY) &gt; &amp;Interval3
			THEN GeneralJournalBalance.AmountRCBalance
		ELSE 0
	END AS D4,
	GeneralJournalBalance.AmountBalance AS TotalD_FCY,
	GeneralJournalBalance.AmountRCBalance AS TotalD
FROM
	AccountingRegister.GeneralJournal.Balance AS GeneralJournalBalance
WHERE
	(GeneralJournalBalance.ExtDimension2 REFS Document.SalesInvoice
			OR GeneralJournalBalance.ExtDimension2 REFS Document.GeneralJournalEntry
				AND GeneralJournalBalance.ExtDimension2.ARorAP = VALUE(Enum.GeneralJournalEntryTypes.AR)
			OR GeneralJournalBalance.ExtDimension2 REFS Document.SalesReturn)
{WHERE
	(GeneralJournalBalance.ExtDimension1 = &amp;Customer)}</query>
