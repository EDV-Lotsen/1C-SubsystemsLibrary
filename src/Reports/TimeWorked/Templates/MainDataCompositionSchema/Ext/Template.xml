		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN TimesheetTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE TimesheetTurnovers.Recorder
	END AS Recorder,
	TimesheetTurnovers.SecondPeriod,
	TimesheetTurnovers.MinutePeriod,
	TimesheetTurnovers.HourPeriod,
	TimesheetTurnovers.DayPeriod,
	TimesheetTurnovers.WeekPeriod,
	TimesheetTurnovers.TenDaysPeriod,
	TimesheetTurnovers.MonthPeriod,
	TimesheetTurnovers.QuarterPeriod,
	TimesheetTurnovers.HalfYearPeriod,
	TimesheetTurnovers.YearPeriod,
	TimesheetTurnovers.Company,
	TimesheetTurnovers.Position,
	TimesheetTurnovers.Employee,
	TimesheetTurnovers.TimeKind AS Kind,
	TimesheetTurnovers.DaysTurnover AS Days,
	TimesheetTurnovers.HoursTurnover AS Hours
FROM
	AccumulationRegister.Timesheet.Turnovers(, , Auto, ) AS TimesheetTurnovers</query>
