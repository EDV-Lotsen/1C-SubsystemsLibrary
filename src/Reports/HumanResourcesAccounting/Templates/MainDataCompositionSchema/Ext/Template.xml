		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN HumanResourcesAccountingBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE HumanResourcesAccountingBalancesAndTurnovers.Recorder
	END AS Recorder,
	HumanResourcesAccountingBalancesAndTurnovers.SecondPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.MinutePeriod,
	HumanResourcesAccountingBalancesAndTurnovers.HourPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.DayPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.WeekPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.TenDaysPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.MonthPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.QuarterPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.HalfYearPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.YearPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.Company,
	HumanResourcesAccountingBalancesAndTurnovers.Employee,
	HumanResourcesAccountingBalancesAndTurnovers.Currency,
	HumanResourcesAccountingBalancesAndTurnovers.AccountingPeriod,
	HumanResourcesAccountingBalancesAndTurnovers.AmountOpeningBalance,
	HumanResourcesAccountingBalancesAndTurnovers.AmountClosingBalance,
	HumanResourcesAccountingBalancesAndTurnovers.AmountTurnover,
	HumanResourcesAccountingBalancesAndTurnovers.AmountReceipt,
	HumanResourcesAccountingBalancesAndTurnovers.AmountExpense,
	HumanResourcesAccountingBalancesAndTurnovers.AmountCurOpeningBalance,
	HumanResourcesAccountingBalancesAndTurnovers.AmountCurClosingBalance,
	HumanResourcesAccountingBalancesAndTurnovers.AmountCurTurnover,
	HumanResourcesAccountingBalancesAndTurnovers.AmountCurReceipt,
	HumanResourcesAccountingBalancesAndTurnovers.AmountCurExpense,
	HumanResourcesAccountingBalancesAndTurnovers.Employee.Code AS TabNumber
FROM
	AccumulationRegister.HumanResourcesAccounting.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS HumanResourcesAccountingBalancesAndTurnovers</query>
